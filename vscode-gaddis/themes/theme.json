{
    "$schema": "vscode://schemas/color-theme",
    "name": "My Gaddis Theme",
    "type": "light",
    "include": "./light_vs.json",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment.line.double-slash.gaddis",
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "TODOs",
            "scope": "keyword.other.todo.gaddis",
            "settings": {
                "foreground": "#40d040"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator.gaddis",
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "name": "Storage",
            "scope": "storage.type.gaddis",
            "settings": {
                "foreground": "#0000AA"
            }
        },
        {
            "name": "Support",
            "scope": "support.function.gaddis",
            "settings": {
                "foreground": "#1111DD"
            }
        },
        {
            "name": "Variables",
            "scope": "variable.other.gaddis",
            "settings": {
                "foreground": "#004000"
            }
        },
        {
            "name": "Comments",
            "scope": "comment.line.semi-colon.gadasm",
            "settings": {
                "foreground": "#B0B0B0"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator.gadasm",
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "name": "Storage",
            "scope": "storage.type.gadasm",
            "settings": {
                "foreground": "#0000AA"
            }
        },
        {
            "name": "Addresses",
            "scope": "constant.numeric.address.gadasm",
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "Labels",
            "scope": "entity.name.label.gadasm",
            "settings": {
                "foreground": "#004000"
            }
        },
        {
            "name": "Variables",
            "scope": "variable.other.register.gadasm",
            "settings": {
                "foreground": "#004000"
            }
        }
    ]
}